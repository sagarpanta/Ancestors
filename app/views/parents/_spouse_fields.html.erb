<fieldset class="spouse_fields">
  <h4>Spouse</h4>
  Full Name<br>
  <%= f.text_field :fullname %><br>
  DOB<br>
  <%= f.date_field :dob %><br>
  Gender<br>
  <%= f.text_field :gender %><br>
  About<br>
  <%= f.text_area :description %><br>
  Upload File</br>
  <%= f.file_field :photo , :class=>"fileinput" %><br><br>
  <%= f.hidden_field :_destroy %>
  <%= link_to "remove", '#', class: "remove_fields" %>
</fieldset>